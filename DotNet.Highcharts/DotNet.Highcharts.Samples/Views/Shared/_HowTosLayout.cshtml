@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/themes/start/jquery-ui-1.10.3.custom.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Scripts/prettify-small-1-Jun-2011/google-code-prettify/prettify.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-2.0.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.6.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Highcharts-3.0.1/js/highcharts.src.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/core.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/prettify-small-1-Jun-2011/google-code-prettify/prettify.js")" type="text/javascript"></script>
</head>
<body onload="prettyPrint()">
    <div class="page">
        <header>
            <div id="title">
                <h1>DotNet.Highcharts</h1>
            </div>
            <div id="logindisplay">
                &nbsp;
            </div>
            <nav>
                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Demo", "BasicLine", "Demo")</li>
                    <li>@Html.ActionLink("How To's", "MultipleXAxes", "HoTos")</li>
                </ul>
            </nav>
        </header>
        <section id="main">
            <div style="float: left; width: 20%">
                <div id="accordion">
                    <h3>
                        <a href="#">How tos</a></h3>
                    <div data-panel-index="0">
                        @Html.ActionLink("Multiple X-Axes", "MultipleXAxes", "HoTos")
                        @Html.ActionLink("Chart inside JavaScript function", "ChartInFunction", "HoTos")
                        @Html.ActionLink("Multiple charts in one page", "MultipleCharts", "HoTos")
                        @Html.ActionLink("Disable Animation", "DisableAnimation", "HoTos")
                        @Html.ActionLink("EaseOutBounce Effect", "EaseOutBounceEffect", "HoTos")
                        @Html.ActionLink("Bind Decimal Values", "BindDecimalValues", "HoTos")
                        @Html.ActionLink("Use .NET Globalization", "UseDotNetGlobalization", "HoTos")
                        @Html.ActionLink("Bind Data From Dictionary", "BindDataFromDictionary", "HoTos")
                        @Html.ActionLink("Bind Data From Object List", "BindDataFromObjectList", "HoTos")
                        @Html.ActionLink("Custom Theme", "CustomTheme", "HoTos")
                        @Html.ActionLink("Tooltip Crosshairs", "TooltipCrosshairs", "HoTos")
                        @Html.ActionLink("Crosshairs With Boolean Values", "CrosshairsWithBooleanValues", "HoTos")
                        @Html.ActionLink("Theming The Reset Button", "ThemingTheResetButton", "HoTos")
                        @Html.ActionLink("Plot Band Events", "PlotBandEvents", "HoTos")
                        @Html.ActionLink("Transparent Background", "TransparentBackground", "HoTos")
                        @Html.ActionLink("Pie Chart With Events", "PieChartWithEvents", "HoTos")
                    </div>
                </div>
            </div>
            <div style="float: left; width: 79%; padding-left: 10px">
                @RenderBody()
            </div>
            <div style="clear: both">
            </div>
        </section>
        <footer>
        </footer>
    </div>
</body>
</html>
