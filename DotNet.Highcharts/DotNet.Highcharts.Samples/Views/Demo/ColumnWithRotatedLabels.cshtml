@model DotNet.Highcharts.Highcharts

@{
    ViewBag.Title = "Column with rotated labels";
}

@(Model)
<script type="text/javascript">
    function TooltipFormatter() {
        return '<b>' + this.x + '</b><br/>' +
                'Population in 2008: ' + Highcharts.numberFormat(this.y, 1) +
                ' millions';
    }
</script>
